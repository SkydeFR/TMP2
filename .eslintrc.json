{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "off",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console": "off"
  }
}